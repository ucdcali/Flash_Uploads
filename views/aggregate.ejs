<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Aggregated Data</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css" rel="stylesheet">
</head>
<body class="bg-light">
  <div class="container py-5">
    <h1 class="text-center mb-4">Aggregated Statistics</h1>
    <table class="table table-bordered">
      <thead>
        <tr>
          <th>Status</th>
          <th>Total Count</th>
          <th>Average Name Length</th>
        </tr>
      </thead>
      <tbody>
        <% aggregationResults.forEach(result => { %>
          <tr>
            <td><%= result._id %></td>
            <td><%= result.total %></td>
            <td><%= result.avgNameLength.toFixed(2) %></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
    <a href="/" class="btn btn-primary mt-3">Back to Examples</a>
  </div>
</body>
</html>
